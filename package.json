{
  "name": "@neosyn-ee/react-native-neos-native-kit",
  "version": "1.3.0",
  "type": "commonjs",
  "description": "A set of ready-to-use components makes app development a fast and effortless experience.",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neosyn-ee/react-native-neos-native-kit"
  },
  "author": "Neosyn <info@neosyn.it> (https://github.com/neosyn-ee)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neosyn-ee/react-native-neos-native-kit/issues"
  },
  "homepage": "https://github.com/neosyn-ee/react-native-neos-native-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "react-devtools": "react-devtools",
    "e2e:ios:build": "detox build --configuration ios.release",
    "e2e:ios:test": "detox test --configuration ios.release",
    "e2e:ios": "run-s e2e:ios:*",
    "e2e:android:build": "detox build --configuration android.release",
    "e2e:android:test": "detox test --configuration android.release",
    "e2e:android": "run-s e2e:android:*",
    "prepare": "husky install",
    "prepack": "bob build"
  },
  "dependencies": {
    "@react-native-camera-roll/camera-roll": "^7.8.1",
    "@react-native-community/datetimepicker": "^6.7.5",
    "@react-native-community/slider": "^4.5.2",
    "@react-native/typescript-config": "0.73.1",
    "@react-navigation/material-bottom-tabs": "^6.2.16",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-native-video": "^5.0.15",
    "react": "18.2.0",
    "react-devtools": "^4.28.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.8",
    "react-native-audio-recorder-player": "^3.6.8",
    "react-native-blob-util": "^0.19.1",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-media-console": "2.1.0",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-paper": "^5.10.3",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.12.0",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-static-safe-area-insets": "^2.2.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-video": "^6.2.0",
    "react-native-vision-camera": "^4.3.2",
    "react-native-web": "^0.18.12",
    "react-singleton-hook": "^4.0.1",
    "semantic-release": "^23.0.8",
    "twrnc": "^3.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-ondevice-actions": "^6.5.1",
    "@storybook/addon-ondevice-backgrounds": "^6.5.1",
    "@storybook/addon-ondevice-controls": "^6.5.1",
    "@storybook/addon-ondevice-notes": "^6.5.1",
    "@storybook/addon-react-native-web": "0.0.19",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.13",
    "@storybook/react-native": "^6.5.1",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.18.9",
    "commitlint-config-jira": "^1.6.4",
    "commitlint-plugin-jira-rules": "^1.6.4",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.28.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.3",
    "metro-react-native-babel-preset": "^0.76.9",
    "prettier": "2.8.8",
    "react-native-builder-bob": "^0.23.2",
    "react-test-renderer": "18.2.0",
    "simple-git": "^3.21.0",
    "tailwindcss": "3.3.2",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ],
    "exclude": "**/{__tests__,__fixtures__,__mocks__,storage,screens}/**"
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
